{
  "name": "@jiaozhiye/qm-design",
  "version": "1.0.0-beta.4",
  "description": "A Component Library for Vue3.0",
  "keywords": [
    "vue3.0",
    "components",
    "typescript"
  ],
  "author": "jiaozhiye",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development BABEL_ENV=web webpack serve --inline --hot --progress --config build/webpack.src.conf.js",
    "build:lib": "yarn clean:lib && yarn build:umd && yarn build:esm && yarn build:type && yarn build:locale && yarn build:theme",
    "build:umd": "cross-env NODE_ENV=production BABEL_ENV=lib webpack --config ./build/webpack.build.conf.js",
    "build:esm": "cross-env BABEL_ENV=lib rollup --config ./build/build-esm.rollup.js",
    "build:type": "gulp build --gulpfile ./build/build-type.gulp.js && cp-cli lib/packages lib && rimraf lib/packages",
    "build:locale": "cross-env BABEL_ENV=utils babel packages/locale --extensions .ts --out-dir lib/locale",
    "build:theme": "gulp build --gulpfile ./build/build-style.gulp.js",
    "clean:lib": "rimraf lib",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "format": "prettier --write \"./src/**/*.{vue,js,ts}\"",
    "website-dev": "cross-env NODE_ENV=development BABEL_ENV=web webpack serve --inline --hot --progress --config ./website/webpack.config.js",
    "website-build": "rimraf website-dist && cross-env NODE_ENV=production BABEL_ENV=web webpack --config ./website/webpack.config.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/*.{js,ts,vue}": [
      "eslint 'src/**/*.{js,ts,vue}'",
      "git add"
    ]
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "style": "lib/style/index.css",
  "peerDependencies": {
    "vue": "^3.0.11"
  },
  "dependencies": {
    "add-dom-event-listener": "^1.1.0",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "countup.js": "^2.0.7",
    "cropperjs": "^1.5.11",
    "dayjs": "^1.10.4",
    "element-plus": "^1.0.2-beta.44",
    "exceljs": "^4.2.1",
    "localforage": "^1.9.0",
    "lodash-es": "^4.17.21",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-into-view-if-needed": "^2.2.28",
    "vue": "^3.0.11",
    "vuedraggable": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.0",
    "@babel/core": "^7.12.0",
    "@babel/plugin-proposal-class-properties": "^7.12.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.12.0",
    "@babel/plugin-transform-runtime": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/component-compiler-utils": "^3.2.0",
    "acorn-jsx": "^5.3.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.0",
    "chalk": "^2.4.2",
    "core-js": "^3.11.0",
    "cp-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.6.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^7.15.0",
    "eslint-plugin-vue": "^7.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-ignore": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "highlight.js": "^10.4.1",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.2",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "mini-css-extract-plugin": "^1.3.6",
    "mitt": "^2.1.0",
    "mockjs": "^1.1.0",
    "ora": "^2.1.0",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.0",
    "rimraf": "^2.7.0",
    "rollup": "^2.28.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.32.0",
    "sass-loader": "^11.0.0",
    "style-resources-loader": "^1.4.0",
    "transliteration": "^2.1.11",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "vue-eslint-parser": "^7.5.0",
    "vue-loader": "^16.2.0",
    "vue-router": "^4.0.6",
    "vue-style-loader": "^4.1.2",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">= 14.16.0",
    "npm": ">= 6.14.0"
  }
}
