{
  "name": "qm-design",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development BABEL_ENV=web webpack serve --inline --hot --progress --config build/webpack.src.conf.js",
    "build:lib": "yarn clean:lib && cross-env NODE_ENV=production BABEL_ENV=lib webpack --config ./build/webpack.build.conf.js && yarn build:type && yarn build:locale && yarn build:theme",
    "build:type": "gulp build --gulpfile ./build/build-type.gulp.js && cp-cli lib/packages lib && rimraf lib/packages",
    "build:rollup": "cross-env BABEL_ENV=lib rollup --config ./build/build-type.rollup.js",
    "build:locale": "cross-env BABEL_ENV=lib babel packages/locale --extensions .ts --out-dir lib/locale",
    "build:theme": "gulp build --gulpfile ./build/build-style.gulp.js",
    "clean:lib": "rimraf lib",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "format": "prettier --write \"./src/**/*.{vue,js,ts}\"",
    "website-dev": "",
    "website-build": ""
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/*.{js,ts,vue}": [
      "eslint 'src/**/*.{js,ts,vue}'",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "style": "lib/style/index.css",
  "dependencies": {
    "add-dom-event-listener": "^1.1.0",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.10.4",
    "element-plus": "^1.0.2-beta.32",
    "localforage": "^1.9.0",
    "lodash-es": "^4.17.20",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-into-view-if-needed": "^2.2.26",
    "vue": "^3.0.6",
    "vuedraggable": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.0",
    "@babel/core": "^7.12.0",
    "@babel/plugin-proposal-class-properties": "^7.12.0",
    "@babel/plugin-proposal-decorators": "^7.12.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.12.0",
    "@babel/plugin-transform-runtime": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vue/babel-plugin-jsx": "^1.0.2",
    "@vue/compiler-sfc": "^3.0.6",
    "@vue/component-compiler-utils": "^3.2.0",
    "acorn-jsx": "^5.3.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.0",
    "chalk": "^2.4.2",
    "core-js": "^3.8.2",
    "cp-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.15.0",
    "eslint-plugin-vue": "^7.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-ignore": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.2",
    "ora": "^2.1.0",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.0",
    "rimraf": "^2.7.0",
    "rollup": "^2.28.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.32.0",
    "sass-loader": "^11.0.0",
    "style-resources-loader": "^1.4.0",
    "svg-sprite-loader": "^5.2.0",
    "ts-loader": "^8.0.10",
    "typescript": "^4.1.2",
    "vue-eslint-parser": "^7.5.0",
    "vue-loader": "^16.1.2",
    "vue-style-loader": "^4.1.2",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  }
}
